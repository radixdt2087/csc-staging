msgctxt "Addons::name::cp_checkout_by_vendor"
msgid "Cart-Power: Checkout by vendor"
msgstr "Cart-Power: Оформление заказа отдельно по вендорам"

msgctxt "Addons::description::cp_checkout_by_vendor"
msgid "Allow for user to checkout by each vendor. Developed by <a target=\"_blank\" href=\"https://cart-power.com/\">Cart-Power</a>"
msgstr "Позволяет офомлять заказы для каждого вендора по очереди. Разработано <a target=\"_blank\" href=\"https://cart-power.ru/\">Cart-Power</a>"

msgctxt "Languages::cp_checkout_this_vendor"
msgid "Pay this vendor"
msgstr "Оформить заказ этого продавца"

msgctxt "Languages::cp_allow_create_payment"
msgid "Allow create payment"
msgstr "Позволить создавать методы оплаты"

msgctxt "Languages::cp_should_checkout_by_vendor"
msgid "Please, select vendor for checkout"
msgstr "Пожалуйста, выберите продавца для оформления заказа"

msgctxt "Languages::cp_payout_product"
msgid "Payout product"
msgstr "Продукт для выплат"

msgctxt "Languages::cp_total_vendor_return"
msgid "Vendors total amount due"
msgstr "Суммарный счет для продавца"

msgctxt "Languages::cp_create_order"
msgid "Create order"
msgstr "Создать заказ"

msgctxt "Languages::cp_vendor_commission"
msgid "Vendor commission"
msgstr "Комиссия вендора"

msgctxt "Languages::cp_paid"
msgid "Paid"
msgstr "Оплачено"

msgctxt "Languages::products_in_session_warn"
msgid "You have some products stored in session. Go to cart page to restore it"
msgstr "У Вас есть продукты в корзине. Перейдите в корзину, чтобы оформить заказ для них."

msgctxt "Languages::products_from_all_vendors_back_in_cart"
msgid "Products, stored in session restored in cart"
msgstr "Продукты в Вашей корзине восстановлены"

msgctxt "SettingsOptions::cp_checkout_by_vendor::allow_all_vendor"
msgid "Allow checkout with all vendors"
msgstr "Позволить оформление заказа для всех продавцов одновременно"

msgctxt "SettingsOptions::cp_checkout_by_vendor::hide_without_payment"
msgid "Do not allow separate checkout if the vendor doesn't have their own payment method"
msgstr "Не разрешать отдельное оформление, если у продавца нет своего способа оплаты"

msgctxt "SettingsOptions::cp_checkout_by_vendor::show_cart_on_complete"
msgid "Show the remaining items in the cart on the order completion page"
msgstr "Показывать оставшиеся товары в корзине на странице завершения заказа"

msgctxt "SettingsOptions::cp_checkout_by_vendor::add_standart_payment"
msgid "Add administrator's payment"
msgstr "Добавлять методы оплаты администратора"

msgctxt "SettingsOptions::cp_checkout_by_vendor::complete_status"
msgid "Order statuses"
msgstr "Статусы заказа"

msgctxt "SettingsOptions::cp_checkout_by_vendor::payout_status"
msgid "Payout order status"
msgstr "Статус заказа для выплат"

msgctxt "Languages::cp_complete_notification"
msgid "You have pending products from the other vendors in your [cart]"
msgstr "У Вас есть продукты других продавцов в корзине [cart]"

msgctxt "SettingsOptions::cp_checkout_by_vendor::licensekey"
msgid "License Key"
msgstr "Лицензионный ключ"

msgctxt "SettingsTooltips::cp_checkout_by_vendor::licensekey"
msgid "Find on the detailed page of your order"
msgstr "Находится на странице Вашего заказа"

msgctxt "Languages::cp_your_license_is_not_valid"
msgid "You should enter valid License Key. Please, contact us (sales@cart-power.com), if you face any issues."
msgstr "Введите корректный лицензионный ключ. Если у Вас возникли трудности с установкой или активацией модуля, пожалуйста, свяжитесь с нами (sales@cart-power.com)"

msgctxt "Languages::cp_not_paid"
msgid "Not paid"
msgstr "Не оплачено"

msgctxt "Languages::cp_cbv_products_from"
msgid "Products from"
msgstr "Товары от"

msgctxt "Languages::cp_cbv_dont_go_to_cart"
msgid "The checkout page was opened in several tabs (devices), please check the correctness of the data in the order before checkout."
msgstr "Страница оформления заказа была открыта в нескольких вкладках (устройствах), пожалуйста, проверьте корректность данных в заказе перед оформлением."


